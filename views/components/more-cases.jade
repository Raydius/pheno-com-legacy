// dynamically populate content from copy.json data
block variables
- page = "moreCases"
- pageData = copy.content[page]

section.col-md-12.no-padding.more-cases 
	span.mono_font More Work Stories
	.more-case-wrapper.cf
		each contentItem in pageData
			article.col-md-4 
				img(src=require("assets/images/wilson_x_2_04-opt.jpg"))
				p!= contentItem.projectName
				cite 
					.wrapper
						span!= contentItem.writerName
						span!= contentItem.writerTitle
							
				a.red(href="#") Read full article