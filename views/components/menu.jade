block variables
	- copy = require('app/copy.js')

nav.nav
	ul
		each navItem in copy.navigation
			li(ng-class="{ selected: isSelected('#{navItem.uiSref}') }", class=navItem.className)
				a.hvr-underline-from-center(ui-sref=navItem.uiSref, href=navItem.uiSref, title=navItem.title, ng-click="closeDrawer()")= navItem.navTitle
